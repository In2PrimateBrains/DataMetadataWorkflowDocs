    "propertyDescriptions": {
      "electrodeID": "Unique identifier for the electrode, to match and be referenced in channel description.",
      "electrodeHemisphere": "Brain hemisphere where the electrode was located.",
      "electrodeLocation": "Textual indication of the location of the electrode (e.g. cortical layer 3, ca1, etc)",
      "electrodeX": "Recorded position along the local x-axis relative to the probe origin and rotation.",
      "electrodeY": "Recorded position along the local y-axis relative to the probe origin and rotation.",
      "electrodeZ": "Recorded position along the local z-axis relative to the probe origin and rotation.",
      "electrodeImpedance": "Impedance of the electrode or pipette (pipette_resistance). This can be a single value or a list of two values indicating a value range.",
      "electrodeShankID": "Identifier to specify which shank of the probe the electrode is present. Alternatively, electrode probe identifier could be used here.",
      "electrodeShape": "Description of the shape of the electrode, e.g. square, circle, etc.",
      "electrodeSize": "Size of the electrode, e.g. non-insulated surface area or length of non-insulated cable.",
      "electrodeMaterial": "Material of the electrode surface.",
      "electrodeInsulationMaterial": "Material used for insulation around the electrode."
    }
  },
