    "propertyDescriptions": {
      "contactID": "Unique identifier for the contact, to match and be referenced in channel description.",
      "contactHemisphere": "Brain hemisphere where the contact was located.",
      "contactLocation": "Textual indication of the location of the contact (e.g. cortical layer 3, ca1, etc)",
      "contactX": "Recorded position along the local x-axis relative to the probe origin and rotation.",
      "contactY": "Recorded position along the local y-axis relative to the probe origin and rotation.",
      "contactZ": "Recorded position along the local z-axis relative to the probe origin and rotation.",
      "contactImpedance": "Impedance of the contact or pipette (pipette_resistance). This can be a single value or a list of two values indicating a value range.",
      "contactShankID": "Identifier to specify which shank of the probe the contact is present.",
      "contactShape": "Description of the shape of the contact, e.g. square, circle, etc.",
      "contactSize": "Size of the contact, e.g. non-insulated surface area or length of non-insulated cable.",
      "contactMaterial": "Material of the contact surface.",
      "contactInsulationMaterial": "Material used for insulation around the contact."
    }
  },
